{
  "name": "mongo",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p mongo",
    "buildDev": "pesy",
    "release": { "releasedBinaries": [ "TicTacToe.exe" ] }
  },
  "buildDirs": {
    "src": {
      "require": [ "bson.lib" ],
      "name": "mongo.lib",
      "namespace": "Mongo",
      "wrapped": false
    },
    "lwt": {
      "require": [ "mongo.lib", "lwt", "lwt.unix" ],
      "preprocess": ["pps", "lwt_ppx"],
      "name": "mongo.lwt",
      "namespace": "Mongo_lwt",
      "wrapped": false
    },
    "test": {
      "require": [ "unix", "mongo.lib" ],
      "main": "run_test",
      "name": "run_test.exe"
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'",
    "test": "esy x run_test.exe"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "bson": "*",
    "@opam/dune": ">=1.6.0",
    "@opam/lwt": "*",
    "@opam/lwt_ppx": "*",
    "ocaml": "^4.4.0"
  },
  "devDependencies": {
    "ocaml": "4.7.1004",
    "refmterr": "*",
    "pesy": "*",
    "@opam/merlin": "*"
  },
  "resolutions": {
    "bson": "EduardoRFS/bson:package.json#556dd475d12097706c0e80a5d21d5d91052a13b8"
  }
}
